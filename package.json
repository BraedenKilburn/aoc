{
  "name": "aoc",
  "version": "1.0.0",
  "author": "Braeden Kilburn",
  "description": "Advent of Code solutions in TypeScript run with Bun.",
  "scripts": {
    "aoc": "bun run index.ts",
    "aoc:generate": "bun run src/generate.ts",
    "aoc:watch": "bun run --watch index.ts",
    "format": "prettier 'src/**/*.ts' --write"
  },
  "devDependencies": {
    "@types/bun": "^1.1.14",
    "prettier": "3.4.2",
    "typescript": "^5.7.2"
  }
}
